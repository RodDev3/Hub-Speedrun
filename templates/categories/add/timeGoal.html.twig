<div>
    <div class='input-group mb-3'>
        <input type='text' class='form-control' name='categories[fields][time-goal.{{ 'now'|date('U') }}][label]'
               placeholder='Title' aria-label='Title'>
    </div>
    <div class='input-group mb-3'>
        <input type='text' class='form-control' name='categories[fields][time-goal.{{ 'now'|date('U') }}][hours]'
               placeholder='' aria-label='' disabled>
